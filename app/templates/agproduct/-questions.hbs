<div class="buildermod__questions buildermod__drawer--questions">
  <h4 class="builder__questions__header">{{t 'graph_builder.builder_questions.land_sown'}}</h4>
  <ul class="builder__questions__list">
    <li class="builder__questions__item">
      {{#link-to 'agproduct.visualization' model.id 'departments' 'treemap' 'land_sown' (query-params startDate=lastYear endDate=lastYear search='null')
        class="icon-cidcon_treemap builder__questions__item__icon"
        tagNmae='i'
      }} {{/link-to}}
      {{#link-to 'agproduct.visualization' model.id 'departments' 'multiples' 'land_sown' (query-params startDate=firstYear endDate=lastYear search='null')
